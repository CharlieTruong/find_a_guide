<%= javascript_include_tag "map/search" %>

<div id="main_search">
  <%= form_tag search_path do %>
    <input id="pac-input" class="controls" type="text" placeholder="Where Are You Going?">
    <%= date_select("show", "date", :default => Date.today, 
      :order => [:month, :day, :year], :start_year => Date.today.year, 
      :end_year => Date.today.year + 3) %>
    <%= date_select("show", "date", :default => Date.today + 1, 
      :order => [:month, :day, :year], :start_year => Date.today.year, 
      :end_year => Date.today.year + 3) %>
  <% end %>
</div>



<p><%= @foo %></p>

<%= javascript_include_tag 'facebook' %>

