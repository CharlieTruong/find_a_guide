<div id="search_results_list">
  <% @users.each do |user| %>
    <div class="search_results_card">
      <%= link_to user_tours_path(user), {:class => 'amb'} do %>
        <div class="ambassador_image" style="background-image: url('<%= user.profile %>')"></div>
        <span class="ambassador_name_card"><h2><%= user.first_name %></h2></span>
        <span class="ambassador_tag_line_card"><%= user.last_name %></span>
        <span class="ambassador_rating_card"><%= user.rating %></span>
      <% end %>
    </div>
  <% end %>
</div>